[flake8]
max-line-length = 119
max-complexity = 12
max-nesting-depth = 12
exclude =
    .venv,
    venv,
    .git,
    _pycache_,
    docs/source/conf.py,
    old,
    build,
    _build,
    dist,
    migrations,
    snapshots

ignore =
    E203,
    E501,
    W503,
    F405,
    E731,
    C901,
    B023